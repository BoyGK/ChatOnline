<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="upper" extends="struts-default" namespace="/upper">

        <action name="login" class="com.chatonline.master.upper.controller.LoginAction" method="execute">
        </action>

        <action name="register" class="com.chatonline.master.upper.controller.RegisterAction" method="execute">
        </action>

        <action name="change_name" class="com.chatonline.master.upper.controller.UserAction" method="execute">
        </action>

        <action name="add_room" class="com.chatonline.master.upper.controller.AddRoomAction" method="execute">
        </action>

        <action name="refash_room" class="com.chatonline.master.upper.controller.GetRoomAction" method="execute">
        </action>
    </package>
</struts>